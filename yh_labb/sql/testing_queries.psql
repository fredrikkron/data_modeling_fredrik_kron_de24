SELECT 
    a.address_id,
    a.street,
    a.postal_code,
    c.city_id,
    c.name
FROM Address a
INNER JOIN yrkesco.city c ON a.city_id = c.city_id;
---------------------------------------------------------------------------

SELECT 
    a.address_id,
    a.street,
    a.postal_code,
    s.school_id,
    s.name
FROM Address a
INNER JOIN yrkesco.school s ON a.address_id = s.address_id;

---------------------------------------------------------------------------